redirect_ipv4 = configRegistry.get("dns/blacklist/redirect/ipv4")
redirect_ipv6 = configRegistry.get("dns/blacklist/redirect/ipv6")
nameserver = "master.ucs.demo" # TODO: GET

print "$TTL\t3600"
print "@\tIN\tSOA\tlocalhost. %s. (" % nameserver
print "\t\t\t 2014052101 \t; Serial"
print "\t\t\t       7200 \t; Refresh"
print "\t\t\t        120 \t; Retry"
print "\t\t\t    2419200 \t; Expire"
print "\t\t\t       3600 )\t; Default TTL"
print ";"
print "@\tIN\tNS\t%s." % nameserver
print "@\t\tA\t%s" % redirect_ipv4
print "@\tIN\tA\t%s" % redirect_ipv4
print "@\t\tAAAA\t%s" % redirect_ipv6
print "@\tIN\tAAAA\t%s" % redirect_ipv6
print ""
