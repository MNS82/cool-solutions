[ssl/usercert/certpath]
Description[de]=Verzeichnis für die Speicherung der Benutzerzertifikate
Description[en]=The directory where to user certificates will be saved
Type=str
Categories=SSL

[ssl/usercert/admingroup]
Description[de]=Diese Gruppe hat Zugriff auf die Benutzerzertifikate (default: "Domain Admins")
Description[en]=This group has access to the user certificates (default: "Domain Admins")
Type=str
Categories=SSL

[ssl/usercert/days]
Description[de]=Anzahl an Tagen die das Benutzerzertifikat gültig ist (default: 1825)
Description[en]=Number of days which the user certificate is valid (default: 1825)
Type=str
Categories=SSL

[ssl/usercert/ca]
Description[de]=Wurzel CA für Benutzerzertifikate (default: ucsCA)
Description[en]=Root CA for user certificates (default: ucsCA)
Type=str
Categories=SSL

[ssl/usercert/sslbase]
Description[de]=SSL Basisverzeichnis (default: /etc/univention/ssl)
Description[en]=SSL base directory (default: /etc/univention/ssl)
Type=str
Categories=SSL

[ssl/usercert/ldapimport]
Description[de]=Benutzerzertifikate am Benutzerobjekt im LDAP speichern (default: yes)
Description[en]=Save user certificates in ldap user object (default: yes)
Type=str
Categories=SSL

[ssl/usercert/certldapmapping/cn]
